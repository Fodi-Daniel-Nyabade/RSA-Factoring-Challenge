#!/usr/bin/python3
import sys

def factorize(n):
    """Factorize a number into the product of two smaller numbers."""
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return (i, n // i)
    return (n, 1)

def main():
    with open(sys.argv[1], 'r') as f:
        for line in f:
            n = int(line)
            p, q = factorize(n)
            print(f"{n}={p}*{q}")

if __name__ == '__main__':
    main()
